{
  "name": "flutter-screen-generator",
  "displayName": "flutter screen generator",
  "description": "",
  "version": "0.0.1",
  "publisher": "MriDx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MriDx/Flutter-Screen-Generator.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "flutter",
    "bloc",
    "generator",
    "screen",
    "dart"
  ],
  "activationEvents": [
    "onCommand:flutter-screen-generator.createScreen",
    "onCommand:flutter-screen-generator.createScreenWithBloc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
      "command": "flutter-screen-generator.createScreen",
      "title": "Flutter Screen Generator: Create Screen"
      },
      {
        "command": "flutter-screen-generator.createScreenWithBloc",
        "title": "Flutter Screen Generator: Create Screen with Bloc"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
